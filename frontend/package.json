{
  "name": "reved-kids-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/react": "^7.120.3",
    "@sentry/tracing": "^7.120.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@types/howler": "^2.2.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/three": "^0.178.1",
    "autoprefixer": "^10.4.16",
    "framer-motion": "^12.23.5",
    "howler": "^2.2.4",
    "lucide-react": "^0.525.0",
    "postcss": "^8.4.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.0",
    "three": "^0.178.0",
    "typescript": "^4.9.5",
    "web-vitals": "^5.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "preview": "serve -s build",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:a11y": "jest --testPathPattern=a11y",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "npm audit --audit-level moderate && npm run test:security:headers",
    "test:security:headers": "node scripts/check-security-headers.js",
    "e2e:test": "playwright test",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:security": "eslint src --ext .ts,.tsx,.js,.jsx --config .eslintrc.security.json",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,css,md}",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "analyze:security": "npm run build && node scripts/security-analysis.js",
    "deploy": "npm run build && npm run deploy:static",
    "deploy:production": "npm run test:security && npm run build && npm run deploy:static",
    "postbuild": "node scripts/post-build-security.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0"
  }
}
